    "minimist": "^1.1.3",
    "pkginfo": "^0.3.0",
    "update-notifier": "^0.5.0",
    "validate.io-array": "^1.0.6",
    "validate.io-function": "^1.0.2",
    "validate.io-matrix-like": "^1.0.2",
    "validate.io-primitive": "^1.0.0",
    "validate.io-string-primitive": "^1.0.0",
    "validate.io-typed-array-like": "^1.0.0"
  },
  "devDependencies": {
    "chai": "3.x.x",
